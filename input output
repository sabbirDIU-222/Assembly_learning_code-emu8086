.model small
.stack 100h
.code
main proc
    mov ah,1
    int 21h        ;input function
    mov bl,al          
    
    
    ; code for new line funciton
    mov ah,2
    mov dl,10
    int 21h   
    mov dl,13
    int 21h
    
    
    
    
    mov ah,2       ; output function
    mov dl,bl
    int 21h
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main
